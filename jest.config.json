{
  "reporters": ["default", ["jest-junit", {
    "outputDirectory": "./coverage"
  }]],
  "coverageThreshold": {
    "global": {
      "functions": 80,
      "lines": 80
    }
  },
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/index.js",
    "!<rootDir>/package.spec.js"
  ],
  "coverageReporters": ["lcov", "text"],
  "testMatch": ["<rootDir>/src/**/?(*.)spec.js"],
  "testEnvironment": "node"
  }
